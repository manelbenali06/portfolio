{% extends 'base.html.twig' %}

{% block title %}projets{% endblock %}

{% block body %}
<header id="portfolio-header">
    <div id="header-img"></div>
    <h1>Manel BENALI</h1>
    <h2>Devellopeur web et web mobile</h2>
</header>
<main class="container-fluid p-5">

    <div class="row">
        <div class="col-12">
            <h2>Mon portfolio</h2>
        </div>
    </div>

    <div class="row">
      
        <div class="col-4 p-3">
            <div class="card">
                    <img src="{{ asset('img/project/HairCare.jpg') }}" class="card-img-top" alt="photo du projet">
                    <div class="card-body">
                        <h4 class="card-title">{{ portfolio.title }}</h5>
                        <p class="card-text">{{ portfolio.description }}</p>
                    </div>
                        
                        
            </div>
    </div>
           
       

</main>

{% endblock %}




